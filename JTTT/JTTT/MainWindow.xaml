<Window x:Class="JTTT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JTTT"
        mc:Ignorable="d"
        Title="JTTT" Height="240" Width="320">
    <Grid>
        <TextBox x:Name="URL" HorizontalAlignment="Left" Height="20" Margin="77,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" Text="Type url adress"/>
        <TextBlock HorizontalAlignment="Left" Margin="37,25,0,0" TextWrapping="Wrap" Text="URL" VerticalAlignment="Top"/>
        <TextBox x:Name="Key" HorizontalAlignment="Left" Height="20" Margin="77,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" TextChanged="TextBox_TextChanged" Text="Type key"/>
        <TextBlock HorizontalAlignment="Left" Margin="37,55,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top"/>
        <TextBox x:Name="Key_Copy" HorizontalAlignment="Left" Height="20" Margin="77,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" TextChanged="TextBox_TextChanged" Text="Type Your e-mail adress"/>
        <TextBlock HorizontalAlignment="Left" Margin="37,109,0,0" TextWrapping="Wrap" Text="Mail" VerticalAlignment="Top"/>
        <CheckBox Content="Send picture" HorizontalAlignment="Left" Margin="37,165,0,0" VerticalAlignment="Top" Checked="CheckBox_Checked"/>
        <Button Content="Push" HorizontalAlignment="Left" Margin="202,165,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
